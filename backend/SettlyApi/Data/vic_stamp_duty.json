{
  "jurisdiction": "VIC",
  "version": "2025-08",
  "effective": { "from": "2008-05-06", "to": null },
  "notes": "If occupancy=PPOR and V<=550k, use PPR table. If V>550k (even PPOR), use General table for the whole V.",
  
  "baseTables": {
    "PPR": [
      { "min": 0, "max": 25000, "formula": { "type": "percentage", "rate": 0.014 } },
      { "min": 25001, "max": 130000, "base": 350, "formula": { "type": "percentage", "rate": 0.024, "onExcess": 25000 } },
      { "min": 130001, "max": 440000, "base": 2870, "formula": { "type": "percentage", "rate": 0.05, "onExcess": 130000 } },
      { "min": 440001, "max": 550000, "base": 18370, "formula": { "type": "percentage", "rate": 0.06, "onExcess": 440000 } }
    ],
    "GENERAL": [
      { "min": 0, "max": 25000, "formula": { "type": "percentage", "rate": 0.014 } },
      { "min": 25001, "max": 130000, "base": 350, "formula": { "type": "percentage", "rate": 0.024, "onExcess": 25000 } },
      { "min": 130001, "max": 960000, "base": 2870, "formula": { "type": "percentage", "rate": 0.06, "onExcess": 130000 } },
      { "min": 960001, "max": 2000000, "formula": { "type": "percentage", "rate": 0.055 } },
      { "min": 2000001, "max": null, "base": 110000, "formula": { "type": "percentage", "rate": 0.065, "onExcess": 2000000 } }
    ]
  },

  "selectors": [
    { "conditions": { "occupancy": "PPOR", "dutiableValueMax": 550000 }, "table": "PPR" },
    { "conditions": {}, "table": "GENERAL" }
  ],

  "modifiers": [
    {
      "id": "vic_fhb_exemption",
      "eligibility": { "isFirstHomeBuyer": true, "dutiableValueMax": 600000 },
      "action": { "type": "override_total_to", "value": 0 },
      "note": "FHB exemption ≤$600,000 (contract on/after 1 Jul 2017)."
    },
    {
      "id": "vic_fhb_concession_600001_750000",
      "eligibility": { "isFirstHomeBuyer": true, "dutiableValueMin": 600001, "dutiableValueMax": 750000 },
      "action": {
        "type": "table_override_total"
      },
      "note": "FHB concession $600,001–$750,000 — sliding scale; table points from SRO examples."
    },
    {
      "id": "vic_foreign_purchaser_additional_duty",
      "eligibility": { "isForeignPerson": true, "propertyType": "residential" },
      "action": { "type": "surcharge_percentage", "rate": 0.08 },
      "note": "FPAD 8% (on or after 1 Jul 2019). If base duty exempt, FPAD does not apply."
    }
  ],

  "rounding": { "mode": "floor_to_dollar" },

  "sources": [
    { "title": "Land transfer duty – PPR (current rates)", "url": "https://www.sro.vic.gov.au/about-us/rates-and-statistics/current-rates/land-transfer-duty-principal-place-residence-current-rates", "checked": "2025-08-17" },
    { "title": "Land transfer duty – non-PPR (general rates)", "url": "https://www.sro.vic.gov.au/about-us/rates-and-statistics/current-rates/land-transfer-duty-non-principal-place-residence-current-rates", "checked": "2025-08-17" },
    { "title": "First home buyer duty exemption/concession", "url": "https://www.sro.vic.gov.au/first-home-owner/exemption-concession-reduction", "checked": "2025-08-17" },
    { "title": "Foreign purchaser additional duty (current rates)", "url": "https://www.sro.vic.gov.au/about-us/rates-and-statistics/current-rates/foreign-purchaser-additional-duty-current-rates", "checked": "2025-08-17" }
  ]
